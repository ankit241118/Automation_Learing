package Sdet_QA_Programs;

public class _28BinarySearch {
	public static void main(String[] args) {
//For binary search array should be sorted
		int s[] = { 33, 55, 80, 95, 130 };
		int search_num= 95;
		
		int high= 0 ;
		int low=s.length-1;
		int mid=(high+low)/2;
		
		while(high>low) {
			if(search_num==mid) {
				System.out.println();
				 	else if(search_num>mid) {
			low=mid+1;
		}
			else if(search_num<mid) {
				high=mid-1;	
		}}}}
